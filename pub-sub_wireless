#subscriber
import zmq
import time
import threading
def subscriber():
    context = zmq.Context()
    sub=context.socket(zmq.SUB)  # Note.
    sub.setsockopt_string(zmq.SUBSCRIBE, 'wireless:')  # Note.
    ip="*"
    sub.connect('tcp://192.168.1.46:5677')
    for i in range(2):
        print('Received: %s' % sub.recv())

threads=threading.Thread(target=subscriber)
threads.start()
